  <para style="q1">
    <verse number="35" style="v" sid="PSA 107:35" />He <char style="w" strong="H7760">turneth</char> the <char style="w" strong="H4057">wilderness</char> into a <char style="w" strong="H0098">standing</char> <char style="w" strong="H4325">water</char>, and <char style="w" strong="H6723">dry</char> <char style="w" strong="H0776">ground</char> into <char style="w" strong="H4325">watersprings</char>.<verse eid="PSA 107:35" /></para>
  <para style="q1">
    <verse number="36" style="v" sid="PSA 107:36" />And there he maketh the <char style="w" strong="H7457">hungry</char> to <char style="w" strong="H3427">dwell</char>, that they may <char style="w" strong="H3559">prepare</char> a <char style="w" strong="H5892">city</char> for <char style="w" strong="H4186">habitation</char>;<verse eid="PSA 107:36" /></para>
