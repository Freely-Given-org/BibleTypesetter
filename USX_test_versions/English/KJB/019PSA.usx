  <para style="q1">
    <verse number="5" style="v" sid="PSA 93:5" />Thy <char style="w" strong="H5713">testimonies</char> are <char style="w" strong="H3966">very</char> <char style="w" strong="H0539">sure</char>: <char style="w" strong="H6944">holiness</char> <char style="w" strong="H4998">becometh</char> thine <char style="w" strong="H1004">house</char>, O <char style="nd"><char style="w" strong="H3068">LORD</char></char>, for <char style="w" strong="H0753">ever</char>.<note caller="+" style="f"><char style="fr" closed="false">93.5 </char><char style="ft" closed="false">for ever: Heb. to length of days</char></note><verse eid="PSA 93:5" /></para>
  <chapter eid="PSA 93" />
