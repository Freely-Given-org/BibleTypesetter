  <para style="q1">
    <verse number="12" style="v" sid="PSA 74:12" />For <char style="w" strong="H0430">God</char> <char style="add">is</char> my <char style="w" strong="H4428">King</char> of <char style="w" strong="H6924">old</char>, <char style="w" strong="H6466">working</char> <char style="w" strong="H3444">salvation</char> in the <char style="w" strong="H7130">midst</char> of the <char style="w" strong="H0776">earth</char>.<verse eid="PSA 74:12" /></para>
  <para style="q1">
    <verse number="13" style="v" sid="PSA 74:13" />Thou didst <char style="w" strong="H6565">divide</char> the <char style="w" strong="H3220">sea</char> by thy <char style="w" strong="H5797">strength</char>: thou <char style="w" strong="H7665">brakest</char> the <char style="w" strong="H7218">heads</char> of the <char style="w" strong="H8577">dragons</char> in the <char style="w" strong="H4325">waters</char>.<note caller="+" style="f"><char style="fr" closed="false">74.13 </char><char style="ft" closed="false">divide: Heb. break</char></note><note caller="+" style="f"><char style="fr" closed="false">74.13 </char><char style="ft" closed="false">dragons: or, whales</char></note><verse eid="PSA 74:13" /></para>
