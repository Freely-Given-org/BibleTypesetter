  <para style="q1">
    <verse number="1" style="v" sid="PSA 94:1" />O <char style="nd"><char style="w" strong="H3068">LORD</char></char> <char style="w" strong="H0410">God</char>, to whom <char style="w" strong="H5360">vengeance</char> belongeth; O <char style="w" strong="H0410">God</char>, to whom <char style="w" strong="H5360">vengeance</char> belongeth, <char style="w" strong="H3313">shew</char> thyself.<note caller="+" style="f"><char style="fr" closed="false">94.1 </char><char style="ft" closed="false">God…: Heb. God of revenges</char></note><note caller="+" style="f"><char style="fr" closed="false">94.1 </char><char style="ft" closed="false">shew…: Heb. shine forth</char></note><verse eid="PSA 94:1" /></para>
  <para style="q1">
