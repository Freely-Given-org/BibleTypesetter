masterfile: 0.0
metadata:
  title: unfoldingWord Literal Text
font:
  family: Libertinus Serif
  size: 10pt
language: en
sile:
  options:
    # class: resilient.book # optional (default)
    papersize: 7.44in x 9.68in   # 189mm x 246mm Crown Quarto
    layout: marginal             # We need big side margins for the notes
    resolution: 300
  settings:
    textsubsuper.fake: true      # Libertinus has decent superscripts, but I prefer the fake ones
